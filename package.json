{
    "name": "marbust-accounts",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
      "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\" \"npm run start-sass\"",
      "start-backend": "cd marbust-accounts-backend && npm start",
      "start-frontend": "cd marbust-accounts-frontend && npm run dev",
      "start-sass": "cd marbust-accounts-frontend/src/sass && npm run sass",
      "sql": "ssh -N -L 3306:127.0.0.1:3306 -p 25552 -l marbusttech marbust.com",
      "install-submodules": "git submodule update --init --recursive"
    },
    "devDependencies": {
      "concurrently": "^7.0.0"
    }
  }